RewriteEngine On

RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.msgestorcontabilonline.com.br/$1 [R,L]

#reescreve files exceto imagens e scripts
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !\.(css|gif|ico|jpg|js|png|swf|txt)$
RewriteRule ^(.*)$ index.php?pagina=$1 [QSA,L]

Options -Indexes
RewriteCond %{HTTP_HOST} ^msgestorcontabilonline\.com\.br$ [OR]
RewriteCond %{HTTP_HOST} ^www\.msgestorcontabilonline\.com\.br$
RewriteRule ^/?$ "https\:\/\/www\.massoncontabilidade\.com\.br\/" [R=301,L]

# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php56” package as the default “PHP” programming language.
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php56 .php .php5 .phtml
</IfModule>
# php -- END cPanel-generated handler, do not edit
